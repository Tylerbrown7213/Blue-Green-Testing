POST https://x.x.x.x/mgmt/shared/appsvcs/declare

{
   "class": "AS3",
   "action": "deploy",
   "persist": true,
   "declaration": {
      "class": "ADC",
      "schemaVersion": "3.41.0",
      "id": "example-declaration-01",
      "label": "Sample 1",
      "remark": "Simple HTTP application with RR pool",
      "blue_green_demo": {
         "class": "Tenant",
         "Application_1": {
            "class": "Application",
            "blue_green_vip1": {
               "class": "Service_HTTP",
               "virtualAddresses": [
                  "x.x.x.y"
               ],
               "allowVlans": [
                  {
                     "bigip": "/Common/external"
                  }
              ],
               "pool": "blue_pool",
               "persistenceMethods": []
             },
            "blue_pool": {
               "class": "Pool",
               "monitors": [
                  "http"
               ],
               "members": [{
                  "servicePort": 80,
                  "serverAddresses": [
                     "x.x.x.b",
                     "x.x.x.b"
                  ]
               }]
            },
            "green_pool": {
              "class": "Pool",
              "monitors": [
                 "http"
              ],
              "members": [{
                 "servicePort": 80,
                 "serverAddresses": [
                    "x.x.x.g",
                    "x.x.x.g"
                 ]
              }]
           }
         }
      }
   }
}

